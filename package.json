  },
  "importSort": {
    ".js, .jsx, .mjs": {
      "parser": "babylon",
      "style": "module"
    },
    ".ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "resolutions": {
    "@arcblock/ux": "^3.1.41",
    "@blocklet/code-editor": "^0.5.20",
    "@blocklet/ui-react": "^3.1.41",
    "@types/d3-dispatch": "3.0.6",
    "@uppy/provider-views": "3.12.0",
    "rimraf": "2.6.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "canvas",
      "core-js",
      "core-js-pure",
      "esbuild",
      "fast-folder-size",
      "leveldown",
      "protobufjs",
      "simple-git-hooks",
      "sqlite3"
    ],
    "overrides": {
      "@arcblock/did-connect": "@arcblock/did-connect-react@^3.1.19"
    }
  }
}
